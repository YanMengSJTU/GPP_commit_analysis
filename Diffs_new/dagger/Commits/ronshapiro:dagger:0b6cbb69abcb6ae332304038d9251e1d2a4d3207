-            console.warn(this._getUndefinedMsg(effectName));
-          }
-        }
-      }, this);
-
-      this._setUpEffect();
-    },
-
-    /**
-     * Forces layout
-     */
-    _layoutIfDirty: function() {
-      return this.offsetWidth;
-    },
