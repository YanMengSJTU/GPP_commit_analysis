-        position: relative;
-      }
-
-      :host([fullbleed]) {
-        @apply --layout-vertical;
-        @apply --layout-fit;
-      }
-
-      :host([fullbleed]) #wrapper,
-      :host([fullbleed]) #wrapper #contentContainer {
-        @apply --layout-vertical;
-        @apply --layout-flex;
