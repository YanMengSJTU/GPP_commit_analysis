-  let scheduled = false;
-  let beforeRenderQueue = [];
-  let afterRenderQueue = [];
-
-  function schedule() {
-    scheduled = true;
-    // before next render
-    requestAnimationFrame(function() {
-      scheduled = false;
-      flushQueue(beforeRenderQueue);
-      // after the render
-      setTimeout(function() {
-        runQueue(afterRenderQueue);
-      });
-    });
