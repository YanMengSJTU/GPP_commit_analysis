-        var type = this.type;
-        var key = this.key;
-
-        if (type && key) {
-          type = IronMeta.types[type] = IronMeta.types[type] || {};
-          if (value == null) {
-            delete type[key];
-          } else {
-            type[key] = value;
-          }
-        }
-      },
-
-      get list() {
-        var type = this.type;
-
-        if (type) {
-          const items = IronMeta.types[this.type];
-          if (!items) {
-            return [];
-          }
-
-          return Object.keys(items).map(function(key) {
-            return metaDatas[this.type][key];
-          }, this);
