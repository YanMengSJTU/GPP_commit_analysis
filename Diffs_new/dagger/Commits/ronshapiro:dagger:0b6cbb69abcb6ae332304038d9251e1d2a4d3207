-  function cubic(a, b, c, d) {
-    if (a < 0 || a > 1 || c < 0 || c > 1) {
-      return linear;
-    }
-    return function(x) {
-      if (x <= 0) {
-        var start_gradient = 0;
-        if (a > 0)
-          start_gradient = b / a;
-        else if (!b && c > 0)
-          start_gradient = d / c;
-        return start_gradient * x;
-      }
-      if (x >= 1) {
-        var end_gradient = 0;
-        if (c < 1)
-          end_gradient = (d - 1) / (c - 1);
-        else if (c == 1 && a < 1)
-          end_gradient = (b - 1) / (a - 1);
-        return 1 + end_gradient * (x - 1);
-      }
