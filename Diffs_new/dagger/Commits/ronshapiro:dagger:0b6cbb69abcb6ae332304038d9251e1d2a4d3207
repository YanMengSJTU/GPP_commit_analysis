-    "web-component-tester": "^6.0.0",
-    "test-fixture": "PolymerElements/test-fixture#3.0.0-rc.1"
-  },
-  "private": true,
-  "resolutions": {
-    "test-fixture": "3.0.0-rc.1"
-  }
-}
