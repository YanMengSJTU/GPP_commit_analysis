-        if (Math.abs(x - xEst) < 0.00001) {
-          return f(b, d, mid);
-        }
-        if (xEst < x) {
-          start = mid;
-        } else {
-          end = mid;
-        }
-      }
-      return f(b, d, mid);
-    }
-  }
