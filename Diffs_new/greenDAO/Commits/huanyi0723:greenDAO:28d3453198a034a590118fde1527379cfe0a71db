+    @Override
+    public void setContentView(int layoutResID) {
+        super.setContentView(layoutResID);
+        initView();
+        initData();
+        setLinstener();
+    }
+
+    protected abstract void initView();
+
+    protected abstract void initData();
+
+    protected abstract void setLinstener();
+
+}
