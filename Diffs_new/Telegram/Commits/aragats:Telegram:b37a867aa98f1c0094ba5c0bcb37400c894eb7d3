-        mInitialScrollOffset = editTextTextPosition - (mSelectorElementHeight * SELECTOR_MIDDLE_ITEM_INDEX);
-        mCurrentScrollOffset = mInitialScrollOffset;
-        updateInputTextView();
-    }
