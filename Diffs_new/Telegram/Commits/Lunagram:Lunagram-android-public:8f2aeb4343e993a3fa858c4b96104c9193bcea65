+        searchbarLine.setBackgroundColor(0xff000000);
+        searchbarLine.setAlpha(0.6f);
+        mainLayout.addView(searchbarLine, new LinearLayout.LayoutParams(LinearLayout.LayoutParams.MATCH_PARENT, 2));
