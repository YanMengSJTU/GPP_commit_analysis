+    public void sendMail(){
+        Intent emailIntent = new Intent(Intent.ACTION_SENDTO, Uri.fromParts(
+                "mailto","telegram@craterzone.com", null));
+        emailIntent.putExtra(Intent.EXTRA_SUBJECT, "Support IM for Telegram");
