+package com.evernote.android.sample;
+
+/**
+ * This is a subclass of ListView, with an implemention of onClick
+ * class created by @akagin_
+ */
+
+import android.content.Context;
+import android.util.AttributeSet;
+import android.view.View;
+import android.widget.ListView;
+
+/**
+ * Custom ListView which tells when a button within in pressed.
+ */
+public class WithButtonListView extends ListView implements View.OnClickListener {
+
+    /**
+     * Constructor
+     */
+    public WithButtonListView(Context ctx){
+        super(ctx);
+    }
+
+    /**
+     * Constructor
+     */
+    public WithButtonListView(Context ctx , AttributeSet attrs){
+        super(ctx , attrs);
+    }
+
+    /**
+     * Called when each button in a list are pushed.
+     * Get Tag embedded on each button.
+     */
+    public void onClick(View view) {
+        int position = (Integer)view.getTag();
+        this.performItemClick(view, position, view.getId());
+    }
+
+}
