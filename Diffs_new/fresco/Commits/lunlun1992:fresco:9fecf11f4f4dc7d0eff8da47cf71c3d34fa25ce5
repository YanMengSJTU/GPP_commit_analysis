+  /**
+   * We get the size from a Kpg image
+   */
+  private Pair<Integer, Integer> readKpgImageSize() {
+    final Pair<Integer, Integer> dimensions = KpgUtil.getSize(getInputStream());
+    if (dimensions != null) {
+      mWidth = dimensions.first;
+      mHeight = dimensions.second;
+    }
+    return dimensions;
+  }
