+    public boolean equals(Object obj) {
+        if (this == obj)
+            return true;
+        if (obj == null)
+            return false;
+
+        if (obj instanceof EntityTypesEvaluationMetrics == false)
+            return false;
+        EntityTypesEvaluationMetrics other = (EntityTypesEvaluationMetrics) obj;
+
